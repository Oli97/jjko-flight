<script>

var canvas = document.getElementById("fest");
var ctx=canvas.getContext("2d");



f = function(x)
{
  return (Math.pow(x,3)-2*Math.pow(x,2));
}




sort = function(x)
{
 var n = x.length;
 var k = [];
 var x1 = x;
 var x2 = [];

for (var l = 0; l < n; l++)
{
  x2[l] = f(x[l]);
}

 for (var l = 0; l < n; l++)
 {
    k[l]=l;
 }
 for (var i = n-1; i>=0; i--)
 {
    for(var j = 1; j<=i; j++)
    {
      if(x2[j-1]>x2[j])
      {
	var temp = x2[j-1];
	x2[j-1] = x2[j];
	x2[j] = temp;

	var temp2 = k[j-1];
	k[j-1] = k[j];
	k[j] = temp2;
      }
    }
 }
 
var kx = [];

for (var l = 0; l < n; l++)
{
  kx[l] = x1[k[l]];
}

 return kx;

}

var x = sort([3,-5,1.5,0,4,2,1]);


ctx.fillText(x,100,50);



</script>


